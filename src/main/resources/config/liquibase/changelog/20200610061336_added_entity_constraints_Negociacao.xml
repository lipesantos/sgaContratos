<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Negociacao.
    -->
    <changeSet id="20200610061336-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="negociacao_id"
                                 baseTableName="negociacao_propostas"
                                 constraintName="fk_negociacao_propostas_negociacao_id"
                                 referencedColumnNames="id"
                                 referencedTableName="negociacao"/>

        <addForeignKeyConstraint baseColumnNames="propostas_id"
                                 baseTableName="negociacao_propostas"
                                 constraintName="fk_negociacao_propostas_propostas_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proposta"/>
        
        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="negociacao"
                                 constraintName="fk_negociacao_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

    </changeSet>
</databaseChangeLog>
